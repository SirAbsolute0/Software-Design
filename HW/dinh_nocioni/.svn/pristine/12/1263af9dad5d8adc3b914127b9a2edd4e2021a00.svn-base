import requests
import re

def getResponse():
    URL = "https://agilec.cs.uh.edu/words"

    return requests.get(URL).text

def parse(response):
    wordList = re.sub(r'[^\w\s]', '', response).split()
    if not all(len(i) == 5 for i in wordList) or len(wordList) == 1:
        raise ValueError("Not a word list")

    return wordList


