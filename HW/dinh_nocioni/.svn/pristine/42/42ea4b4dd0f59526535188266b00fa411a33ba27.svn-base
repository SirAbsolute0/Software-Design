class Wordle: #Feedback: YAGNI, please remove this line
  target = "FAVOR" #Feedback: please remove
  
  def submitRow(self, row): #Feedback: def tally(target, guess)
    charCodes = list()
    for i in range(len(row)):
      if(row[i] in self.target):
        if(row[i] == self.target[i]):
          charCodes.append(2) #Same position #Feedback: please remove such comments. We will write code so it is self expressive
        else:
          charCodes.append(1) #Different position
      else:
        charCodes.append(0) #Character not in target
    
    return charCodes
