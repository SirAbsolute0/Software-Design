import enum

class Match(enum.Enum):
  EXACT = "EXACT"
  EXISTS = "EXISTS"
  NO_MATCH = "NO_MATCH"


def tally(target, guess):
  
  if(len(guess) != 5):
    raise ValueError("Incorrect Size")

  response = list()
  
  for i in range(len(guess)):
      
    if(guess[i] in target):
      if(guess[i] == target[i]):
        response.append(Match.EXACT)  
      else:
        response.append(Match.EXISTS) 
    else:
      response.append(Match.NO_MATCH)
      
  return response
